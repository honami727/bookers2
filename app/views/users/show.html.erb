<div>
    <h3><%= @user.name %></h3>
    <%= image_tag @user.get_profile_image(100,100) %>
</div>

<% @post_images.each do |post_image| %>

<div>
    <p><%= image_tag @user.get_profile_image(100,100) %></p>
    <p><%= post_image.book_name %></p>
    <p><%= post_image.caption %></p>
    <p><%= post_image.user.name %></p>
</div>

<% end %>